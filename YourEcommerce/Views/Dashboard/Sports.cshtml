@using YourEcommerce.DTOs.SportDtos
@model YourEcommerce.ViewModels.TableDashboardViewModel<SportDto>
@{
    ViewData["Title"] = "Deportes";
}

<div class="w-full p-4 overflow-auto">
    @await Component.InvokeAsync("TableDashboard", new { 
        title = Model.Title,
        items = Model.Items,
        total = Model.TotalItems,
        url = Model.ApiBaseUrl
    })
</div>